  clock-names = "baudclk", "apb_pclk";
  reg-shift = <2>;
  reg-io-width = <4>;
  dmas = <&dmac 6>, <&dmac 7>;
  #dma-cells = <2>;
  pinctrl-names = "default";
  pinctrl-0 = <&uart3m1_xfer &uart3m1_cts &uart3m1_rts>;
