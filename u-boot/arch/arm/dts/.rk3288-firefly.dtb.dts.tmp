 cpus {
  #address-cells = <1>;
  #size-cells = <0>;
  enable-method = "rockchip,rk3066-smp";
  rockchip,pmu = <&pmu>;

  cpu0: cpu@500 {
   device_type = "cpu";
   compatible = "arm,cortex-a12";
   reg = <0x500>;
   operating-points = <

    1800000 1400000
    1704000 1350000
    1608000 1300000
    1512000 1250000
    1416000 1200000
    1200000 1100000
    1008000 1050000
     816000 1000000
     696000 950000
     600000 900000
     408000 900000
     216000 900000
     126000 900000
   >;
   #cooling-cells = <2>;
   clock-latency = <40000>;
   clocks = <&cru 6>;
   resets = <&cru 0>;
  };
  cpu@501 {
   device_type = "cpu";
   compatible = "arm,cortex-a12";
   reg = <0x501>;
   resets = <&cru 1>;
  };
  cpu@502 {
   device_type = "cpu";
   compatible = "arm,cortex-a12";
   reg = <0x502>;
   resets = <&cru 2>;
  };
  cpu@503 {
   device_type = "cpu";
   compatible = "arm,cortex-a12";
   reg = <0x503>;
   resets = <&cru 3>;
  };
 };

